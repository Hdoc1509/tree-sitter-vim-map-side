" === keycodes ===
<c-w>]<c-w>T
" <- @punctuation.bracket
"^^^ @character.special
"   ^ @punctuation.bracket
"     ^ @punctuation.bracket
"      ^^^ @character.special
"         ^ @punctuation.bracket

" === <cmd> with pipe ===
<cmd>call CustomFunction() \| OtherCommand<CR>
" <- @punctuation.bracket
"^^^ @character.special
"   ^ @punctuation.bracket
"                          ^^ @string.escape
"                                         ^ @punctuation.bracket
"                                          ^^ @character.special
"                                            ^ @punctuation.bracket

" === : with pipe ===
:call CustomFunction() \| OtherCommand\|EventMoreCommands<CR>
"                      ^^ @string.escape
"                                     ^^ @string.escape
"                                                        ^ @punctuation.bracket
"                                                         ^^ @character.special
"                                                           ^ @punctuation.bracket

" === printf ===
printf('m`%so<esc>``', v:count1)
" <- @function.call
"     ^ @punctuation.bracket
"      ^^^^^^^^^^^^^^ @string
"                    ^ @punctuation.delimiter
"                              ^ @punctuation.bracket
