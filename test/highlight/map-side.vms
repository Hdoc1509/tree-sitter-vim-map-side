" === keycodes ===
<c-w>]<c-w>T
" <- @punctuation.bracket
"^^^ @character.special
"   ^ @punctuation.bracket
"     ^ @punctuation.bracket
"      ^^^ @character.special
"         ^ @punctuation.bracket

" === <cmd> with pipe ===
<cmd>call CustomFunction() \| OtherCommand<CR>
" <- @character.special
" <- !@punctuation.bracket
"^^^^ @character.special
"   ^ !@punctuation.bracket
"                          ^^ @string.escape
"                                         ^ !@punctuation.bracket
"                                         ^^^^ @character.special
"                                            ^ !@punctuation.bracket

" === <cmd> with <bar> ===
<cmd>call CustomFunction() <bar> OtherCommand<CR>
" <- @character.special
" <- !@punctuation.bracket
"^^^^ @character.special
"   ^ !@punctuation.bracket
"                          ^ !@punctuation.bracket
"                          ^^^^^ @operator
"                              ^ !@punctuation.bracket
"                                            ^ !@punctuation.bracket
"                                            ^^^^ @character.special
"                                               ^ !@punctuation.bracket


" === <cmd> with pipe in lua string ===
<cmd>tabnew | Git<cr><c-w>o
" <- @character.special
" <- !@punctuation.bracket
"^^^^ @character.special
"   ^ !@punctuation.bracket
"                ^ !@punctuation.bracket
"                ^^^^ @character.special
"                   ^ !@punctuation.bracket
"                    ^ @punctuation.bracket
"                     ^^^ @character.special
"                        ^ @punctuation.bracket

" === : between keycodes ===
<c-w><cr>:cclose<cr><c-w>T
" <- @punctuation.bracket
"^^^ @character.special
"   ^^ @punctuation.bracket
"    ^ ! @character.special
"     ^^ @character.special
"       ^ !@character.special
"       ^ @punctuation.bracket
"               ^ !@punctuation.bracket
"               ^^^^ @character.special
"                  ^ !@punctuation.bracket
"                   ^ @punctuation.bracket
"                    ^^^ @character.special
"                       ^ @punctuation.bracket

" === : with pipe ===
:call CustomFunction() \| OtherCommand\|EventMoreCommands<CR>
"                      ^^ @string.escape
"                                     ^^ @string.escape
"                                                        ^ !@punctuation.bracket
"                                                        ^^^^ @character.special
"                                                           ^ !@punctuation.bracket

" === lua current line ===
:.lua<cr>
"    ^ !@punctuation.bracket
"    ^^^^ @character.special
"       ^ !@punctuation.bracket
